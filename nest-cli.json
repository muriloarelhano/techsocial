{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api-users/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "plugins": [
      "@nestjs/swagger"
    ],
    "webpack": true,
    "tsConfigPath": "apps/api-users/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/api-users",
  "projects": {
    "api-users": {
      "type": "application",
      "root": "apps/api-users",
      "entryFile": "main",
      "sourceRoot": "apps/api-users/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-users/tsconfig.app.json"
      }
    },
    "api-products": {
      "type": "application",
      "root": "apps/api-products",
      "entryFile": "main",
      "sourceRoot": "apps/api-products/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-products/tsconfig.app.json"
      }
    }
  }
}